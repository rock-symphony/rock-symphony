{
    "name": "rock-symphony/rock-symphony",
    "description": "Fork of symfony 1.4 with dic, form enhancements, latest swiftmailer and better performance",
    "license": "MIT",
    "require": {
        "php": ">=5.4.0 <7.3",
        "swiftmailer/swiftmailer": "~5.2",
        "rock-symphony/container": "^3.1.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "psr/log": "*"
    },
    "type": "library",
    "autoload": {
        "files": ["lib/SYMFONY_VERSION.php"],
        "classmap": ["lib"],
        "exclude-from-classmap": [
            "/lib/command/cli.php",
            "/lib/config/config",
            "/lib/controller/default",
            "/lib/exception/data",
            "/lib/helper",
            "/lib/i18n/data",
            "/lib/task/generator/skeleton",
            "/lib/test/w3"
        ]
    },
    "suggest": {
        "rock-symphony/sf-doctrine-plugin": "Doctrine1 plugin",
        "propel/sf-propel-o-r-m-plugin": "Propel plugin",
        "psr/log": "Psr logger"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-1.6.x": "1.6-dev",
            "dev-1.5.x": "1.5-dev"
        }
    },
    "bin": ["data/bin/symfony"]
}
