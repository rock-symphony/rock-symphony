{
    "name": "rock-symphony/rock-symphony",
    "description": "Fork of symfony 1.4 with dic, form enhancements, latest swiftmailer and better performance",
    "license": "MIT",
    "require": {
        "php": ">=5.6.0 <7.3",
        "swiftmailer/swiftmailer": "~5.2",
        "symfony/yaml": "^3.4|^4.0"
    },
    "require-dev": {
        "psr/log": "*",
        "nette/robot-loader": "^3.1"
    },
    "provide": {
        "rock-symphony/plugin-contract": "2.0"
    },
    "type": "library",
    "autoload": {
        "files": ["lib/SYMFONY_VERSION.php"],
        "classmap": ["lib"],
        "exclude-from-classmap": [
            "/lib/command/cli.php",
            "/lib/config/config",
            "/lib/controller/default",
            "/lib/exception/data",
            "/lib/helper",
            "/lib/i18n/data",
            "/lib/task/generator/skeleton",
            "/lib/test/w3"
        ]
    },
    "suggest": {
        "propel/sf-propel-o-r-m-plugin": "Propel plugin",
        "psr/log": "Psr logger"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev",
            "dev-5.x": "5.x-dev",
            "dev-4.x": "4.x-dev",
            "dev-3.x": "3.x-dev",
            "dev-1.6.x": "1.6-dev",
            "dev-1.5.x": "1.5-dev"
        }
    },
    "bin": ["data/bin/symfony"]
}
